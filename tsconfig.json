{
    "extends": "./.svelte-kit/tsconfig.json",
    "exclude": [
        "node_modules",
        "dist",
        "out",
        "electron"
    ],
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true
    }
}